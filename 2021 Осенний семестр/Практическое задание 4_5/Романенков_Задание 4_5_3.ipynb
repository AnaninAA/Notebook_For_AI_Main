    "import numpy as np\n",
    "import pandas\n",
    "import matplotlib.pyplot\n",
    "\n",
    "df1 = pandas.read_csv('winequality-white.csv', ';')\n",
    "df2 = df1[df1['residual sugar'] < 1]\n",
    "df2 = df2[['pH', 'density']]\n",
    "df2 = df2[df2.index % 2 == 0]\n",
    "\n",
    "print('==================')\n",
    "df2"
